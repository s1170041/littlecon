<p ><%= notice %></p>

<h1>Message Details</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @message_details.each do |message_detail| %>
      <tr>
        <td><%= link_to 'Show', message_detail %></td>
        <td><%= link_to 'Edit', edit_message_detail_path(message_detail) %></td>
        <td><%= link_to 'Destroy', message_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Message Detail', new_message_detail_path %>
